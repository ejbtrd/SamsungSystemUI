<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/notification_panel_theme_ripple_color"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="@dimen/qs_edit_icon_radius" />
            <stroke android:width="1.0dip" android:color="@color/qs_edit_button_icon_color" />
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape>
            <corners android:radius="@dimen/qs_edit_icon_radius" />
            <solid android:color="@android:color/white" />
        </shape>
    </item>
</ripple>